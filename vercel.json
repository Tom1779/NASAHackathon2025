{
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/sbdb",
      "destination": "https://ssd-api.jpl.nasa.gov/sbdb.api"
    },
    {
      "source": "/api/neo",
      "destination": "https://ssd-api.jpl.nasa.gov/neo.api"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
